{
  "name": "color-fns",
  "version": "0.1.1",
  "description": "Modern JavaScript color utility library.",
  "main": "dist/color-fns.js",
  "module": "dist/color-fns.esm.js",
  "unpkg": "dist/color-fns.js",
  "typings": "dist/types/index.d.ts",
  "repository": "https://github.com/baianat/color-fns",
  "license": "MIT",
  "keywords": [
    "color",
    "utility",
    "functions"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint './src/**/*.ts' --fix",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "types": "tsc --declaration --declarationDir ./types --emitDeclarationOnly ./src/index.ts"
  },
  "maintainers": [
    {
      "name": "Abdelrahman3D",
      "email": "abdelrahman3d@gmail.com"
    }
  ],
  "files": [
    "dist/**/*.js",
    "dist/types/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^7.6.1",
    "@types/jest": "^24.0.13",
    "chalk": "^2.4.2",
    "filesize": "^4.1.2",
    "gzip-size": "^5.1.1",
    "husky": "^2.3.0",
    "jest": "24.8.0",
    "lint-staged": "^8.1.7",
    "mkdirp": "^0.5.1",
    "rollup": "^1.12.5",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1",
    "uglify-js": "^3.5.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "tslint --fix",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ]
  }
}
